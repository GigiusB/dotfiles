<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>net.devpi</string>

        <key>EnvironmentVariables</key>
        <dict>
            <key>PYTHONPATH</key>
            <string></string>
        </dict>

        <key>ProgramArguments</key>
        <array>
            <string>/data/VENV/sax/bin/python</string>
            <string>/data/VENV/sax/bin/devpi-server</string>

            <string>--host</string>
